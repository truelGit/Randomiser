<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Randomiser.NewVariant">
    <ContentPage.Content>
        <StackLayout Orientation="Vertical">
            <Entry Placeholder="Enter Variant" x:Name="variant"/>
            <Button x:Name="VariantAdded" Clicked="VariantAdded_Clicked" Text="Ok"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>